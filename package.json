{
  "name": "bern-ssr",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "api": "bookmonkey-api",
    "build:universal": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:universal": "node dist/server.js",
    "build:static": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:static",
    "serve:static": "cd dist/browser && http-server",
    "build:client-and-server-bundles": "ng build --prod && ng build --prod --app 1 --output-hashing=false",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "generate:static": "cd dist && node prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.0-rc.3",
    "@angular/common": "^6.0.0-rc.3",
    "@angular/compiler": "^6.0.0-rc.3",
    "@angular/core": "^6.0.0-rc.3",
    "@angular/forms": "^6.0.0-rc.3",
    "@angular/http": "^6.0.0-rc.3",
    "@angular/platform-browser": "^6.0.0-rc.3",
    "@angular/platform-browser-dynamic": "^6.0.0-rc.3",
    "@angular/platform-server": "^6.0.0-rc.3",
    "@angular/router": "^6.0.0-rc.3",
    "@angular/service-worker": "^6.0.0-rc.3",
    "@nguniversal/express-engine": "^5.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0",
    "core-js": "^2.5.5",
    "express": "^4.16.2",
    "rxjs": "^6.0.0-rc.1",
    "rxjs-compat": "^6.0.0-beta.1",
    "ts-loader": "^3.1.1",
    "xmlhttprequest": "^1.8.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "http-server": "~0.11.1",
    "bookmonkey-api": "^2.0.3",
    "@angular/cli": "^1.7.4",
    "@angular/compiler-cli": "^6.0.0-rc.3",
    "@angular/language-service": "^6.0.0-rc.3",
    "typescript": "~2.7.2",
    "@types/node": "^8.0.50"
  }
}
